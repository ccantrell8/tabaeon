{
  "manifest_version": 2,
  "name": "TabAeon",
  "description": "This extension will cycle through open tabs.",
  "version": "1.0",
  "browser_action": {
    "default_icon": {
    "19": "icons/icon19.png",
    "38": "icons/icon38.png"
    },
  "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "tabs",
    "alarms"
  ],
  "background": {
    "scripts": ["js/app/background.js"],
    "persistent": false
  }
}